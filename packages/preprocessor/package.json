{
    "name": "@svelte-daisyui/preprocessor",
    "version": "1.0.0",
    "author": "5exyGuy (AurÄ—jus Remeika)",
    "license": "MIT",
    "main": "index.js",
    "types": "index.d.ts",
    "files": [
        "index.js",
        "index.d.ts",
        "binding.js",
        "binding.d.ts"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "scripts": {
        "artifacts": "napi artifacts",
        "build": "napi build --platform --release --js binding.js --dts binding.d.ts --pipe \"prettier -w\"",
        "build:debug": "napi build --platform --js binding.js  --dts binding.d.ts --pipe \"prettier -w\"",
        "prepublishOnly": "napi prepublish -t npm",
        "version": "napi version"
    },
    "napi": {
        "name": "preprocessor",
        "triples": {
            "defaults": true
        }
    },
    "engines": {
        "node": ">= 10"
    },
    "devDependencies": {
        "@napi-rs/cli": "^2.12.0",
        "prettier": "^2.7.1",
        "prettier-plugin-rust": "^0.1.8"
    }
}
