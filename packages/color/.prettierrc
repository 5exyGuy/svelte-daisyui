{
    "useTabs": false,
    "printWidth": 80,
    "tabWidth": 4,
    "singleQuote": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "bracketSpacing": true,
    "semi": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "proseWrap": "always",
    "requirePragma": false,
    "insertPragma": false
}
