{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "package/**", ".svelte-kit/**"]
        },
        "dev": {
            "dependsOn": ["^dev"],
            "outputs": ["dist/**", "package/**", ".svelte-kit/**"]
        }
    }
}
